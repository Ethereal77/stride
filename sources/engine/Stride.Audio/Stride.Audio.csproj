<Project>
  <PropertyGroup>
    <StrideNativeOutputName>libstrideaudio</StrideNativeOutputName>
    <StrideRuntime>true</StrideRuntime>
  </PropertyGroup>
  <Import Project="..\..\targets\Stride.props" />
  <Import Project="Sdk.props" Sdk="MSBuild.Sdk.Extras" Version="3.0.22" />
  <PropertyGroup>
    <StrideAssemblyProcessor>true</StrideAssemblyProcessor>
    <StridePlatformDependent>true</StridePlatformDependent>
    <StrideBuildTags>*</StrideBuildTags>
    <StrideCodeAnalysis>true</StrideCodeAnalysis>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyTitle>Stride Audio Engine</AssemblyTitle>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="..\..\shared\SharedAssemblyInfo.cs">
      <Link>Properties\SharedAssemblyInfo.cs</Link>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Stride.Native\Stride.Native.csproj" />
    <ProjectReference Include="..\Stride\Stride.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Native\Celt.cpp" />
    <None Include="Stride.Native.Libs.targets">
      <SubType>Designer</SubType>
    </None>
  </ItemGroup>
  <ItemGroup>
    <None Include="Native\Common.h" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Native\XAudio2.cpp" />
  </ItemGroup>
  <Import Project="$(StrideSdkTargets)" />
  <Import Project="Sdk.targets" Sdk="MSBuild.Sdk.Extras" Version="3.0.22" />
</Project>
